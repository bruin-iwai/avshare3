{
  "name": "avshare3",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --check **/*.{js,ts,vue}",
    "format": "prettier --write **/*.{js,ts,vue}",
    "lint": "npm run lint --workspaces",
    "start": "npm run start -w @avshare3/api",
    "test": "jest",
    "prepare": "husky install",
    "testall": "npm run format && npm run lint && npm test",
    "deploy:gui": "npm run generate -w @avshare3/gui && aws s3 sync packages/gui/dist/ s3://${BUCKET_FRONTEND}/",
    "deploy:api": "npm run sls -w @avshare3/api -- deploy"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@nuxt/eslint": "^0.3.13",
    "@types/basic-auth": "^1.1.4",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@types/tsscmp": "^1.0.2",
    "aws-sdk-client-mock": "^4.0.1",
    "esbuild": "^0.19.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.3",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "serverless": "^3.35.2",
    "serverless-esbuild": "^1.48.0",
    "serverless-prune-plugin": "^2.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite-plugin-vuetify": "^1.0.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
