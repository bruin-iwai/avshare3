{
  "name": "avshare3",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --check **/*.{js,ts,vue}",
    "format": "prettier --write **/*.{js,ts,vue}",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.vue .",
    "start": "npm run start -w @avshare3/api",
    "test": "jest",
    "prepare": "husky install",
    "testall": "npm run format && npm run lint && npm test",
    "deploy:gui": "npm run generate -w @avshare3/gui && aws s3 sync packages/gui/dist/ s3://${BUCKET_FRONTEND}/",
    "deploy:api": "npm run sls -w @avshare3/api -- deploy"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@types/basic-auth": "^1.1.4",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@types/tsscmp": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-sdk-client-mock": "^3.0.0",
    "esbuild": "^0.19.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.3",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "serverless": "^3.35.2",
    "serverless-esbuild": "^1.48.0",
    "serverless-prune-plugin": "^2.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite-plugin-vuetify": "^1.0.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
