{
  "name": "avshare3",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --check **/*.{js,ts,vue}",
    "format": "prettier --write **/*.{js,ts,vue}",
    "lint": "npm run lint --workspaces",
    "start": "npm run start -w @avshare3/api",
    "test": "jest",
    "prepare": "husky install",
    "testall": "npm run format && npm run lint && npm test",
    "deploy:gui": "npm run generate -w @avshare3/gui && aws s3 sync packages/gui/dist/ s3://${BUCKET_FRONTEND}/",
    "deploy:api": "npm run sls -w @avshare3/api -- deploy"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nuxt/eslint": "^0.4.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "aws-sdk-client-mock": "^4.0.1",
    "esbuild": "^0.19.12",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.12.4",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "serverless": "^3.39.0",
    "serverless-esbuild": "^1.52.1",
    "serverless-prune-plugin": "^2.0.2",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^2.0.29"
  },
  "workspaces": [
    "packages/*"
  ]
}
