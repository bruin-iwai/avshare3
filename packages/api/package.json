{
  "name": "@avshare3/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/standalone.ts",
    "lint": "npx eslint .",
    "sam": "sam",
    "sam:build": "sam build",
    "sam:deploy": "sam deploy --no-confirm-changeset --no-fail-on-empty-changeset",
    "builddeploy": "run-s sam:build custombuild sam:deploy",
    "buildlocal": "node build.mjs",
    "startlocal": "node dist/index.js",
    "custombuild": "./build.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@fastify/compress": "^8.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@h4ad/serverless-adapter": "^4.4.0",
    "fastify": "^5.2.1"
  }
}
